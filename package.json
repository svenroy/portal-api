{
  "name": "portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "starts": "npm run build && DEBUG=app node ./dist/server",
    "start": "DEBUG=app* nodemon --exec npm run babel-node -- ./server",
    "test": "npm run build && mocha --recursive",
    "lint": "eslint server.js",
    "babel-node": "babel-node --presets=env --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "cookie-parser": "~1.4.3",
    "debug": "^2.6.9",
    "express": "^4.16.3",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "swagger-ui-express": "^3.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2"
  }
}
