    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "restify": "^7.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0",
    "restify-clients": "^2.0.2",
    "rimraf": "^2.6.2"
